{
  "name": "clipster",
  "displayName": "Clipster",
  "version": "1.1.33",
  "description": "A VS Code extension to copy paths, folder structures, and contents with customization.",
  "main": "src/extension.js",
  "activationEvents": [
    "onCommand:clipster.copyFileContentWithHeader",
    "onCommand:clipster.copyFolderStructure",
    "onCommand:clipster.copyFolderStructureAndContent",
    "onCommand:clipster.copyRootFolderPath",
    "onCommand:clipster.copyRootFolderStructure",
    "onCommand:clipster.copyRootFolderStructureAndContent"
  ],
  "publisher": "jesperwilfing",
  "scripts": {
    "clean": "rimraf ./out",
    "build": "npx @vscode/vsce package",
    "bump-build": "standard-version && npm run build",
    "clean-build-install": "npm run clean && npm run bump-build && npm run install-extension",
    "install-extension": "node ./build.js",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheJesper/clipster"
  },
  "keywords": [
    "vscode",
    "extension",
    "clipboard",
    "root path",
    "folder structure",
    "file content"
  ],
  "author": "jesperwilfing",
  "license": "MIT",
  "devDependencies": {
    "@vscode/vsce": "^2.15.0",
    "standard-version": "^9.3.2",
    "rimraf": "^3.0.2"
  },
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-menu-test.test1",
        "title": "Test 1"
      },
      {
        "command": "vscode-menu-test.test2",
        "title": "Test 2"
      },
      {
        "command": "vscode-menu-test.test3",
        "title": "Test 3"
      },
      {
        "command": "vscode-menu-test.test4",
        "title": "Test 4"
      }
    ],
    "submenus": [
      {
        "id": "sample.editor.context",
        "label": "Test"
      },
      {
        "id": "sample.explorer.context",
        "label": "Test"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "sample.editor.context",
          "group": "test"
        }
      ],
      "explorer/context": [
        {
          "submenu": "sample.explorer.context",
          "group": "test"
        }
      ],
      "sample.editor.context": [
        {
          "command": "vscode-menu-test.test1"
        },
        {
          "command": "vscode-menu-test.test2"
        },
        {
          "command": "vscode-menu-test.test3"
        },
        {
          "command": "vscode-menu-test.test4"
        }
      ],
      "sample.explorer.context": [
        {
          "command": "vscode-menu-test.test1"
        },
        {
          "command": "vscode-menu-test.test2"
        },
        {
          "command": "vscode-menu-test.test3"
        }
      ]
    }
  }
}
